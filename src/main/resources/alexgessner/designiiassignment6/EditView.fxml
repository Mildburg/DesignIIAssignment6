<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="alexgessner.designiiassignment6.Controllers.EditController">
   <children>
      <TableView fx:id="preWorkoutTableView" layoutX="112.0" layoutY="133.0" prefHeight="66.0" prefWidth="380.0">
        <columns>
          <TableColumn fx:id="idClm" onEditCommit="#idChanged" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="nameClm" onEditCommit="#nameChanged" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="cafClm" onEditCommit="#cafChanged" prefWidth="75.0" text="Caffeine" />
            <TableColumn fx:id="lCitClm" onEditCommit="#lCitChanged" prefWidth="75.0" text="L-Citrulline" />
            <TableColumn fx:id="betaAlanClm" onEditCommit="#betaAlanChanged" text="Beta-Alanine" />
        </columns>
      </TableView>
      <VBox layoutX="250.0" layoutY="200.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="44.0" prefWidth="100.0">
               <children>
                  <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submitEdit" text="Submit" />
               </children>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefHeight="46.0" prefWidth="100.0">
               <children>
                  <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitToMain" text="Exit" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
